{
  "name": "unplugin-json-dts",
  "version": "0.0.5",
  "description": "Automatically generate better typings for json files",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "tsup src/*.ts && esno scripts/postbuild.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flixcor/unplugin-json-dts.git"
  },
  "keywords": [
    "json",
    "vite",
    "unplugin",
    "plugin",
    "typescript",
    "rollup",
    "webpack"
  ],
  "author": "Felix Cornelissen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flixcor/unplugin-json-dts/issues"
  },
  "homepage": "https://github.com/flixcor/unplugin-json-dts#readme",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "esno": "^0.10.1",
    "fast-glob": "^3.2.7",
    "path": "^0.12.7",
    "tsup": "^5.5.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "unplugin": "^0.2.19"
  }
}
